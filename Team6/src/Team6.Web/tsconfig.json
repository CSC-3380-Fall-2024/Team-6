{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./wwwroot/js",
      "rootDir": "./wwwroot/js/TypeScript",
      "sourceMap": true
    },
    "include": [
      "wwwroot/js/TypeScript/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
  }